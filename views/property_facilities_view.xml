<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="property_facilities_action" model="ir.actions.act_window">
        <field name="name">Property Facilities</field>
        <field name="res_model">property.facilities</field>
        <field name="view_mode">list</field>
    </record>
    <record id="property_facilities_view_tree" model="ir.ui.view">
        <field name="name">property.facilities.list</field>
        <field name="model">property.facilities</field>
        <field name="arch" type="xml">
            <list string="Property Facilities" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>
</odoo>